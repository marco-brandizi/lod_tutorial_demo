PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX atlasterms:  <http://rdf.ebi.ac.uk/resource/atlas/>

SELECT DISTINCT ?protein ?condition 
WHERE {
  ?speciman 
    atlasterms:dbXref ?protein;
    atlasterms:hasFactorValue ?condition.
    
  ?condition rdfs:subClassOf ?inputCondition
}
ORDER BY rand()
LIMIT 100

